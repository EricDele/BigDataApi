{
  "api": {
    "getUsers": "/service/xusers/users",
    "getUserByName": "/service/xusers/users?name={{ userName }}",
    "getUserById": "/service/xusers/users/{{ userId }}",
    "getGroups": "/service/xusers/groups",
    "getGroupByName": "/service/xusers/groups?name={{ groupName }}",
    "getGroupById": "/service/xusers/groups/{{ groupId }}",
    "createUser": "/service/xusers/secure/users",
    "setRoleForUser": "/service/xusers/secure/users/{{ id }}",
    "deleteUser": "/service/xusers/users/{{ id }}?forceDelete=true",
    "getServiceById": "/service/public/v2/api/service/{{ serviceId }}",
    "getServiceByName": "/service/public/v2/api/service/name/{{ serviceName }}",
    "getServices": "/service/public/v2/api/service{% if serviceType is defined %}?serviceType={{ serviceType }}{% endif %}",
    "getPolicyByServiceAndPolicyName": "/service/public/v2/api/service/{{ serviceName }}/policy/{{ policyName }}",
    "getSearchPolicyInService": "/service/public/v2/api/service/{{ serviceName }}/policy{% if policyName is defined %}?policyName={{ policyName }}{% endif %}",
    "postCreatePolicy": "/service/public/v2/api/policy",
    "postApplyPolicy": "/service/public/v2/api/policy/apply",
    "putUpdatePolicyByServiceAndPolicyName": "/service/public/v2/api/service/{{ serviceName }}/policy/{{ policyName }}",
    "deleteDeletePolicyByServiceAndPolicyName": "/service/public/v2/api/policy{% if serviceName is defined %}?servicename={{ serviceName }}&policyname={{ policyName }}{% endif %}"
  }
}